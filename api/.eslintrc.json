{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": ["warn", { "code": 120 }],
        "indent": ["error", 4],
        "new-cap": 1,
        "require-jsdoc": ["warn"]
    }
}
